<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bg="urn:import:org.gwtbootstrap3.client.ui.gwt">

	<b:Container paddingTop="10" width="100%">
		<b:Panel width="100%">
			<b:PanelHeader>
				<b:Row>
					<b:Column size="XS_12,SM_12,MD_8,LG_8">
						<b:Heading size="H4" ui:field="title" />
					</b:Column>
					<b:Column size="XS_12,SM_12,MD_4,LG_">
						<b:ButtonGroup pull="RIGHT" ui:field="bgRight">
							<b:Button type="PRIMARY" icon="PLUS" ui:field="btnAdd"></b:Button>
							<b:Button type="PRIMARY" icon="REFRESH" ui:field="btnReload"></b:Button>
						</b:ButtonGroup>
					</b:Column>
				</b:Row>
			</b:PanelHeader>
			<b:PanelBody>
				<bg:CellTable ui:field="grid" width="100%" condensed="true" hover="true" striped="true" />
				<b:Pagination ui:field="pagination" pull="RIGHT" />
			</b:PanelBody>
		</b:Panel>
	</b:Container>
