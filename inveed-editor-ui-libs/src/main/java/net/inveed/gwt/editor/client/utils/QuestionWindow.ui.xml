<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">

	<ma:overlay.MaterialOverlay ui:field="overlay" textAlign="CENTER" textColor="WHITE">
		<m:MaterialLabel ui:field="lblTitle" textAlign="CENTER" marginTop="120" fontWeight="LIGHTER" fontSize="2em" />
		<m:MaterialLabel ui:field="lblText" textAlign="CENTER" marginBottom="40" />
		<m:MaterialButton margin="8" type="OUTLINED" backgroundColor="RED" textColor="WHITE"  ui:field="btnOK" text="OK" />
		<m:MaterialButton margin="8" type="OUTLINED" backgroundColor="BLACK" textColor="WHITE" ui:field="btnCancel" text="Cancel" />
		
	</ma:overlay.MaterialOverlay>
</ui:UiBinder>